{
  "name": "react-saga-webpack-arc",
  "version": "0.0.1",
  "description": "react saga webpack arc",
  "main": "server.js",
  "repository": {
    "type": "git"
  },
  "author": "Gaoyuan",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov npm run test && nyc report",
    "test": "eslint app test && NODE_PATH=$NODE_PATH:$PWD/app ava | tap-nyan",
    "pack": "rm -f node_modules.tar.bz2 && tar -cvf node_modules.tar node_modules/ && bunzip2 -z node_modules.tar",
    "deploy": "bunzip2 node_modules.tar.bz2 && tar -xvf node_modules.tar && rm -f node_modules.tar",
    "start:dev": "NODE_ENV=development babel-node server",
    "start:demo": "NODE_ENV=demo babel-node server",
    "start:prod": "NODE_ENV=production babel-node server",
    "build": "NODE_ENV=production webpack --config ./webpack.production.config.js --profile --colors",
    "build:dev": "NODE_ENV=development webpack --config ./webpack.config.js --profile --colors",
    "build:demo": "NODE_ENV=demo webpack --config ./webpack.production.config.js --profile --colors",
    "eslint": "eslint app test --fix",
    "jscs": "jscs .",
    "test:simple": "ava"
  },
  "dependencies": {
    "antd": "^2.9.1",
    "create-react-class": "^15.5.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery-param": "^0.2.0",
    "less": "^2.7.2",
    "prop-types": "^15.5.8",
    "react": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.14.3",
    "trim": "0.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.2.0",
    "ava": "^0.18.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.1.1",
    "babel-jscs": "^3.0.0-beta1",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "browser-env": "^2.0.21",
    "clean-webpack-plugin": "^0.1.5",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^6.9.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.7.0",
    "jscs": "^3.0.7",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "koa": "~1.2.4",
    "koa-bodyparser": "~2.3.0",
    "koa-gzip": "~0.1.0",
    "koa-http-proxy": "0.0.1",
    "koa-route": "~2.4.2",
    "koa-send": "^3.1.0",
    "koa-static": "~2.0.0",
    "koa-webpack-dev-middleware": "^1.1.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-command-params": "^0.1.0",
    "node-sass": "^4.3.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-redux": "^4.0.7",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.1.1",
    "sinon": "^1.16.1",
    "stats-webpack-plugin": "^0.4.3",
    "style-loader": "^0.13.1",
    "tap-nyan": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-hot-middleware": "^2.16.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "app/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "failFast": false,
    "tap": false,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
